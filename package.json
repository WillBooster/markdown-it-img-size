    "markdown-it": "12.0.4",
    "parcel-bundler": "1.12.4",
    "prettier": "2.2.1",
    "shelljs": "0.8.4",
    "shx": "0.3.3",
    "sort-package-json": "1.49.0",
    "ts-jest": "26.5.1",
    "ts-node": "9.1.1",
    "typescript": "4.1.5"
  }
