  "author": "WillBooster LLC",
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "shx rm -Rf dist && shx mkdir dist",
    "build": "parcel build src/index.ts --target=node",
    "format": "yarn lint-fix && yarn prettier && yarn sort-package-json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "yarn lint --fix",
    "prettier": "prettier --write {,*/}**/*.{css,htm,html,json,md,scss,yaml,yml}",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "update-fixture": "ts-node script/FixtureGenerator.ts"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@types/image-size": "0.7.0",
    "image-size": "0.7.4",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/markdown-it": "0.0.8",
    "@types/node": "12.0.12",
    "@types/shelljs": "0.8.5",
    "@willbooster/eslint-config": "0.1.0",
    "@willbooster/prettier-config": "0.1.0",
    "@willbooster/renovate-config": "0.1.0",
    "@willbooster/tsconfig": "0.0.13",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "lint-staged": "9.0.2",
    "markdown-it": "8.4.2",
    "parcel-bundler": "1.12.3",
    "shelljs": "0.8.3",
    "shx": "0.3.2",
    "sort-package-json": "1.22.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.2"
  }
}
