  ],
  "scripts": {
    "prebuild": "shx rm -Rf dist && shx mkdir dist",
    "build": "parcel build src/index.ts --target=node",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettier",
    "lint": "eslint \"./{packages/*/,}{src,__tests__}/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prettier": "prettier --write \"**/{.*/,}*.{css,htm,html,js,json,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "update-fixture": "ts-node script/FixtureGenerator.ts"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "image-size": "0.8.3"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/markdown-it": "0.0.9",
    "@types/node": "13.13.0",
    "@types/shelljs": "0.8.7",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "@willbooster/eslint-config-ts": "3.0.0",
    "@willbooster/prettier-config": "3.0.0",
    "@willbooster/renovate-config": "3.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jest": "25.3.0",
    "lint-staged": "10.1.6",
    "markdown-it": "10.0.0",
    "parcel-bundler": "1.12.4",
    "prettier": "2.0.4",
    "shelljs": "0.8.3",
    "shx": "0.3.2",
    "sort-package-json": "1.42.0",
    "ts-jest": "25.4.0",
    "ts-node": "8.8.2",
    "typescript": "3.8.3"
  }
