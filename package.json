  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "shx rm -Rf dist && shx mkdir dist",
    "build": "parcel build src/index.ts --target=node",
    "format": "yarn lint-fix && yarn prettier && yarn sort-package-json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "yarn lint --fix",
    "prettier": "prettier --write {,*/}**/*.{css,htm,html,json,md,scss,yaml,yml}",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "update-fixture": "ts-node script/FixtureGenerator.ts"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@types/image-size": "0.7.0",
    "image-size": "0.7.4",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/markdown-it": "0.0.9",
    "@types/node": "12.7.8",
    "@types/shelljs": "0.8.5",
    "@typescript-eslint/eslint-plugin": "2.3.2",
    "@typescript-eslint/parser": "2.3.2",
    "@willbooster/eslint-config": "1.0.0",
    "@willbooster/prettier-config": "1.0.0",
    "@willbooster/tsconfig": "1.0.0",
    "eslint": "6.5.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.0.7",
    "jest": "24.9.0",
    "lint-staged": "9.4.0",
    "markdown-it": "10.0.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "shelljs": "0.8.3",
    "shx": "0.3.2",
    "sort-package-json": "1.22.1",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "typescript": "3.6.3"
  }
}
